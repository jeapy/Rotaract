{% extends "JPCotisationBundle:Default:index.html.twig" %}
  {% block cotisation %} 
          <div class="box box-danger">
                <div class="box-header with-border">
                  <h3 class="box-title">Ajouter une Cotisation</h3>
                  <div class="box-tools"> 
                      <div class="box-tools">                    
                          <div class="btn-group"> 

                            <a href="{{ path('cotisation_index') }}" class="btn btn-sm btn-default"><i class="fa fa-ban "></i> Annuler</a>
                          </div>                                     
                      </div>
                    </div>
                </div>
                <div class="box-body">
                   {{ form_start(edit_form) }}              

                  {{ form_errors(edit_form) }}

    
 <div class="row">
   <div class="col-md-6">
                     {{ form_label(edit_form.typecotisation) }}
                      {{ form_errors(edit_form.typecotisation) }}
                       <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user-o"></i></span>                       
                        {{ form_widget(edit_form.typecotisation, {'attr': {'class': 'form-control', 'placeholder':''}}) }}
                      </div>
                </div>
                   
                <div class="col-md-6">
                {{ form_label(edit_form.montant) }}
                      {{ form_errors(edit_form.montant) }}
                       <div class="input-group">                      
                        <span class="input-group-addon"><i class="fa fa-user-o"></i></span>                       
                        {{ form_widget(edit_form.montant, {'attr': {'class': 'form-control', 'placeholder':''}}) }}
                      </div>
                </div>
               
              
              </div>   
  
 <br>
                         
                      
                             
                 {{ form_end(edit_form) }}
                </div><!-- /.box-body -->
              </div><!-- /.box -->


    {% endblock %}


